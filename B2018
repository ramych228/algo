    rep(i, n){
        int l, r;
        cin >> l >> r;
        pos[i][0] = l, pos[i][1] = r, pos[i][2] = i;
    }

    sort(all(pos));
    int mx = 0;

    for(auto &u: pos){
        int l = u[0], r = u[1], t = u[2];
        if(mx >= r) res[t] = {-1, -1};
        else{
            res[t] = {max(l, mx), r};
            mx = r;
        }
    }

    for(auto u: res) cout << u.F << ' ' << u.S << '\n';
