#include "bits/stdc++.h"
using namespace std;

#define rep(i, n) for(int i = 0; i < n; i++)
#define forn(i, n) for(int i = 1; i <= n; i++)

#define ull unsigned long long
#define int long long
#define all(v) v.begin(), v.end()
#define pb push_back
#define S second
#define F first
typedef double db;
typedef long double ld;
